#floatdiv
{
    position: fixed;
    top: 260px;
    background-color: black;
    text-align: center;
    width: auto;
    height: 50%;
    list-style-type: none;
    left:63%;
}

#ftable
{
    font-family:"Trebuchet MS", Arial, Helvetica, sans-serif;
    width:100%;
    height: auto;
    border-collapse:collapse;
    font-size:1em;
    list-style-type: none;
}

